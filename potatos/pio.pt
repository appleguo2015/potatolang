// aPpLegUo
// Standard Potato IO Library

// Print a value followed by a newline
fun pio_println(x) {
  print x;
}

// Print a value without a newline (using write builtin)
fun pio_print(x) {
  write(x);
}

// Return the entire input as a string
fun pio_read_all() {
  if (len(input) > 0) {
    return input;
  }
  let content = "";
  let line = read_line();
  while (line != nil) {
    content = content + line + "\n";
    line = read_line();
  }
  return content;
}

// Read a single line from input
fun pio_read_line() {
  return read_line();
}
